{
  "productName": "Snip Tuck",
  "version": "0.0.1",
  "description": "A code snippet manager",
  "main": "main.js",
  "scripts": {
    "hot-server": "node -r babel-register server.js",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djwirz/sniptuck.git"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/djwirz/sniptuck/blob/master/README.md",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "concurrently": "^2.2.0",
    "electron": "^1.3.4",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.2.1",
    "express": "^4.14.0",
    "fbjs-scripts": "^0.7.1",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.3.1",
    "redux-logger": "^2.6.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "electron-debug": "^1.0.1",
    "mongoose": "^4.6.1",
    "morgan": "^1.7.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.2"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
